<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mowozy.Entity.Users" table="users" schema="yes">
        <id name="uId">
            <column name="UId" sql-type="int(11)"/>
        </id>
        <property name="password">
            <column name="Password" sql-type="text" not-null="true"/>
        </property>
        <property name="grade">
            <column name="Grade" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="authority">
            <column name="Authority" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="uaId">
            <column name="UAId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="email">
            <column name="Email" sql-type="text" not-null="true"/>
        </property>
        <property name="uName">
            <column name="UName" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>