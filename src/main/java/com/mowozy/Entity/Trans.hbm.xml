<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mowozy.Entity.Trans" table="trans" schema="yes">
        <id name="tId">
            <column name="TId" sql-type="int(11)"/>
        </id>
        <property name="tStartDate">
            <column name="TStartDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="tuId">
            <column name="TUId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="tPrice">
            <column name="TPrice" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="tEndDate">
            <column name="TEndDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="tState">
            <column name="TState" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="tImage">
            <column name="TImage" sql-type="text" not-null="true"/>
        </property>
        <property name="tName">
            <column name="TName" sql-type="text" not-null="true"/>
        </property>
        <property name="tNum">
            <column name="TNum" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>